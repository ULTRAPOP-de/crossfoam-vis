{
  "name": "@crossfoam/vis",
  "version": "1.0.3",
  "description": "visualisation base class",
  "main": "dst/index.js",
  "scripts": {
    "lint": "tslint -p tslint.json --force",
    "build": "tsc",
    "typescript": "npm run lint && npm run build",
    "watch": "nodemon "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultrapop-de/crossfoam-vis.git"
  },
  "author": "sebastian-meier @seb_meier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ultrapop-de/crossfoam-vis/issues"
  },
  "homepage": "https://github.com/ultrapop-de/crossfoam-vis#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "dst/*"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run typescript",
    "ext": "ts"
  },
  "dependencies": {
    "@crossfoam/data": "^1.0.3",
    "@crossfoam/ui-helpers": "^1.0.8",
    "@crossfoam/utils": "^1.0.2",
    "@pixi/unsafe-eval": "^5.0.4",
    "a-color-picker": "^1.1.8",
    "d3": "^5.11.0",
    "pixi.js": "^5.1.2",
    "regl": "^1.3.13"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.3",
    "nodemon": "^1.19.4",
    "tslint": "^5.20.1",
    "typescript": "^3.9.3",
    "web-ext-types": "^3.1.0"
  }
}
